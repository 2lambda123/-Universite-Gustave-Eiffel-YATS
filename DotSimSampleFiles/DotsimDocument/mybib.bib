@TECHREPORT{AICPub540:1988,
  author = {Bratman, Michael E. and Israel, David J. and Pollack, Martha E.},
  title = {Plans and Resource-Bounded Practical Reasoning},
  institution = {AI Center, SRI International},
  year = {1988},
  number = {425},
  address = {333 Ravenswood Ave., Menlo Park, CA 94025},
  month = {Sep},
  note = {Revised version.},
  abstract = {An architecture for a rational agent must allow for means-end reasoning,
	for the weighing of competing alternatives, and for interactions
	between these two forms of reasoning. Such an architecture must also
	address the problem of resource boundedness. We sketch a solution
	of the first problem that points the way to a solution of the second.
	In particular, we present a high-level specification of the practical-reasoning
	component of an architecture for a resource-bounded rational agent.
	In this architecture, a major role of the agent’s plans is to constrain
	the amount of further practical reasoning she must perform.},
  file = {:C\:\\DotSim\\Article\\bratman-israel-pollack-1988-clearer.pdf:PDF},
  keywords = {Planning!Resource-bounded, Rational Agents!Planning}
}

